<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_hop_dong_draft_reminder" model="ir.cron">
            <field name="name">Nhắc hợp đồng nháp</field>
            <field name="model_id" ref="model_hop_dong"/>
            <field name="state">code</field>
            <field name="code">model._cron_send_draft_reminders()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
        </record>

        <record id="ir_cron_hop_dong_approval_reminder" model="ir.cron">
            <field name="name">Nhắc duyệt hợp đồng</field>
            <field name="model_id" ref="model_hop_dong"/>
            <field name="state">code</field>
            <field name="code">model._cron_send_approval_reminders()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
        </record>

        <record id="ir_cron_hop_dong_effective_reminder" model="ir.cron">
            <field name="name">Nhắc lịch ký trước hiệu lực</field>
            <field name="model_id" ref="model_hop_dong"/>
            <field name="state">code</field>
            <field name="code">model._cron_send_effective_reminders()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
        </record>

        <record id="ir_cron_hop_dong_check_expiry" model="ir.cron">
            <field name="name">Kiểm tra hết hạn hợp đồng</field>
            <field name="model_id" ref="model_hop_dong"/>
            <field name="state">code</field>
            <field name="code">model._cron_check_expiry()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
        </record>
    </data>
</odoo>
